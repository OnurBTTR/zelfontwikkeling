{
  "name": "webeau/skeleton",
  "license": "proprietary",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://gitlab.com/api/v4/group/8319839/-/packages/composer/packages.json"
    }
  ],
  "require": {
    "php": "8.3.*",
    "bttr/sofie": "^11.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "bttr/sofie-dev": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer fix",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage-html storage/app/report",
    "phpstan": "vendor/bin/phpstan analyse --memory-limit=512M",
    "phpstan-baseline": "vendor/bin/phpstan analyse --memory-limit=512M --generate-baseline --allow-empty-baseline",
    "pre-install-cmd": [
      "Webeau\\Sofie\\Console\\ComposerScripts::preInstall"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "Webeau\\Sofie\\Console\\ComposerScripts::postInstall"
    ],
    "pre-update-cmd": [
      "Webeau\\Sofie\\Console\\ComposerScripts::preUpdate"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "Webeau\\Sofie\\Console\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "Webeau\\Sofie\\Console\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-merge": [
      "@composer dump-autoload -o",
      "Webeau\\Sofie\\Console\\ComposerScripts::postMerge"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "preferred-install": "dist",
    "process-timeout": 0,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
